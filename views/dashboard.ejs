<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Pgina Principal Usuarios</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">
    <link rel="stylesheet" href=".././css/styles.css">
  </head>
  <body class="container">
    <%-include('partials/navdash') %>
    <div class="row">
      <div class="col-sm-5 ">
        <div class="card">
          <div class=" card-body btn btn-outline-dark">
            <h4 class="card-title">Bienvenido a la Plataforma</h4>
            <h5 class="card-text">Usuario : <%= user.username %></h5>
            
          </div>
        </div>
      </div>
      <div class="col-sm-6">
        <div class="card">
          <div class="card-body btn btn-outline-dark ">
            <h5 class="card-title">Email : </h5>
            <h5 class="card-text"><%= user.email %></h5>            
          </div>
        </div>
      </div>
    </div>
    <div class="alert alert-warning">
      <%- include('partials/somos')%>
    </div>
    

</body>
</html>
  